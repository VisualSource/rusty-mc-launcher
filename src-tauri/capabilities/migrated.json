{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPDATA/**"
      ]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "windows-open-file",
          "cmd": "explorer",
          "args": [
            {
              "validator": "[a-zA-Z]:\\\\.+"
            }
          ]
        }
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://user.auth.xboxlive.com/user/authenticate"
        },
        {
          "url": "https://xsts.auth.xboxlive.com/xsts/authorize"
        },
        {
          "url": "https://api.minecraftservices.com/authentication/login_with_xbox"
        },
        {
          "url": "https://api.minecraftservices.com/minecraft/profile"
        },
        {
          "url": "https://api.minecraftservices.com/minecraft/profile/capes/active"
        },
        {
          "url": "https://api.minecraftservices.com/minecraft/profile/skins"
        },
        {
          "url": "https://api.minecraftservices.com/minecraft/profile/skins/active"
        }
      ]
    },
    "core:window:allow-request-user-attention",
    "core:webview:allow-webview-size",
    "core:webview:allow-set-webview-size",
    "core:path:allow-resolve-directory",
    "core:window:allow-toggle-maximize",
    "core:window:allow-is-maximizable",
    "core:window:allow-start-dragging",
    "core:window:allow-is-maximized",
    "core:window:allow-is-minimized",
    "core:window:allow-outer-size",
    "core:window:allow-inner-size",
    "core:app:allow-tauri-version",
    "core:window:allow-unminimize",
    "core:window:allow-unmaximize",
    "core:window:allow-maximize",
    "core:window:allow-minimize",
    "core:window:allow-set-size",
    "core:window:allow-close",
    "core:app:allow-version",
    "core:window:allow-show",
    "core:window:allow-hide",
    "core:path:allow-join",
    "core:app:allow-name",
    "core:event:default",
    "fs:allow-write-file",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "dialog:allow-message",
    "dialog:allow-confirm",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-ask",
    "process:allow-restart",
    "process:allow-exit",
    "deep-link:default",
    "shell:allow-open",
    "updater:default",
    "log:default",
    "rmcl-auth:default",
    "rmcl-content:default",
    "rmcl-game:default",
    "rmcl-query:default",
    "stronghold:default"
  ]
}